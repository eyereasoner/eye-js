{
  "preset": "ts-jest",
  "collectCoverage": true,
  "coverageReporters": [
    "json-summary", 
    "text",
    "lcov"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "collectCoverageFrom": [ "dist/**" ]
}
