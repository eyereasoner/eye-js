{
  "name": "eyereasoner",
  "version": "0.0.0",
  "description": "Distributing the [EYE](https://github.com/josd/eye) reasoner for browser and node using WebAssembly.",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test": "jest __tests__/*-test.ts",
    "lint": "eslint lib/**/*.ts __tests__/* scripts/* --ext .ts",
    "lint:fix": "eslint lib/**/*.ts __tests__/* scripts/* --ext .ts --fix",
    "build": "npm run eye:prepare && tsc",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "eye:fetch": "ts-node scripts/fetch-eye",
    "eye:pvm": "ts-node scripts/generate-pvm",
    "eye:build": "rollup --config rollup.config.mjs",
    "eye:prepare": "npm run eye:fetch && npm run eye:build",
    "eye:update": "ts-node scripts/update",
    "perf": "ts-node perf/socrates"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eyereasoner/eye-js.git"
  },
  "keywords": [
    "eye",
    "eye-js",
    "reasoning",
    "reasoner",
    "rdf",
    "owl",
    "semantic",
    "surfaces",
    "solid"
  ],
  "author": "Jesse Wright <https://github.com/jeswr/>",
  "contributors": [
    "Jesse Wright <https://github.com/jeswr/>",
    "Jos De Roo <https://github.com/josd/>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eyereasoner/eye-js/issues"
  },
  "homepage": "https://github.com/eyereasoner/eye-js#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@types/jest": "^29.2.4",
    "@types/n3": "^1.10.4",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-rdf": "^1.7.1",
    "pre-commit": "^1.2.2",
    "rollup": "^3.8.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "eye": {
      "name": "v23.0105.1853",
      "sha": "53a896e2bfdaff3b2de19493769563cc6a38c410"
    }
  },
  "dependencies": {
    "n3": "^1.16.3",
    "swipl-wasm": "^3.0.0"
  },
  "peerDependencies": {
    "@rdfjs/types": "^1.1.0"
  }
}
